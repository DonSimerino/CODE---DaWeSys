{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "0.8",
  "manifest_version": 3,
  "permissions": [ "cookies"],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["popup.js"]
    }
  ],
  "background":{
    "service_worker":
      "background.js"
  },
  "action": {
    "default_popup": "extension.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}